  "dependencies": {
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "react": "^0.14.3",
    "react-router-component": ">=0.27",
    "react-router-component-transition": "^0.2.0",
    "react-addons-css-transition-group": "^0.14.1",
