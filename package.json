    "reactify": "^1.1.1",
    "uglify-js": "^2.6.1"
  },
  "scripts": {
    "build-dev": "browserify -t [ babelify --presets [react] ] src/main.js -o client/js/bundle.js",
    "build-prod": "browserify -t [ babelify --presets [react] ] src/main.js | uglifyjs > client/js/bundle.min.js",
    "watch": "watchify -t [ babelify --presets [react] ] src/main.js -o client/js/bundle.js",
    "clean-js": "rm public/*.js"
  }
